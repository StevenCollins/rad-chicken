version 27
__lua__
-- rad chicken
-- rubber chicken studios
function _init()
 -- set transparency.
	palt(0,false)
	palt(15,true)
	
	make_chicken()
	make_ground()
	make_obstacles()
end

function _update()
 move_ground()
 move_chicken()
 move_obstacles()
end

function _draw()
	-- clear screen to bg colour.
	cls(1)
	-- draw everything else.
	draw_ground()
	draw_chicken()
	draw_obstacles()
end
